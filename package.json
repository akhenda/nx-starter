{
  "name": "@nx-starter/source",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "volta": {
    "node": "22.6.0",
    "bun": "1"
  },
  "scripts": {
    "preinstall": "npx only-allow bun",
    "prepare": "husky",
    "gc": "git add . && cz",
    "gc-ai": "git add . && npx czg ai -N=5",
    "deps:audit": "npx bun-audit",
    "deps:update:1": "bunx nx run-many --target=update-deps --all",
    "deps:update:2": "bunx npm-check-updates --root --format group -i",
    "deps:update:3": "bunx taze --interactive --recursive",
    "deps:update:4": "bunx npm-upgrade"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git",
      "useEmoji": true
    }
  },
  "dependencies": {
    "axios": "1.7.3",
    "koa": "2.15.3",
    "tslib": "2.6.3"
  },
  "devDependencies": {
    "@commitlint/cli": "19.4.0",
    "@commitlint/config-conventional": "19.2.2",
    "@commitlint/config-nx-scopes": "19.3.1",
    "@nx/esbuild": "19.5.7",
    "@nx/eslint": "19.5.7",
    "@nx/eslint-plugin": "19.5.7",
    "@nx/jest": "19.5.7",
    "@nx/js": "19.5.7",
    "@nx/node": "19.5.7",
    "@nx/workspace": "19.5.7",
    "@swc-node/register": "1.10.9",
    "@swc/cli": "0.4.0",
    "@swc/core": "1.7.10",
    "@swc/helpers": "0.5.12",
    "@swc/jest": "0.2.36",
    "@theunderscorer/nx-semantic-release": "2.11.0",
    "@types/jest": "29.5.12",
    "@types/koa": "2.15.0",
    "@types/node": "22.2.0",
    "@typescript-eslint/eslint-plugin": "8.0.1",
    "@typescript-eslint/parser": "8.0.1",
    "cz-git": "1.9.4",
    "esbuild": "0.23.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "husky": "9.1.4",
    "jest": "29.7.0",
    "jest-environment-node": "29.7.0",
    "lint-staged": "15.2.8",
    "nx": "19.5.7",
    "prettier": "2.8.8",
    "ts-jest": "29.2.4",
    "ts-node": "10.9.2",
    "typescript": "5.5.4",
    "verdaccio": "5.32.1"
  },
  "nx": {
    "includedScripts": []
  }
}
