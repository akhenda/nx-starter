    "@eslint/js": "9.9.1",
    "@nx-starter/eslint-config": "workspace:*",
    "@nx/esbuild": "19.6.3",
    "@nx/eslint": "19.6.3",
    "@nx/eslint-plugin": "19.6.3",
    "@nx/jest": "19.6.3",
    "@nx/js": "19.6.3",
